<h2 class="text-center"> Upload CSV file to analyze </h2>
<div>
<%= form_for @email_list, url: email_lists_path do |f| %>
    <div class="form-group">
      <%= f.label :name, "List Name" %>
      <%= f.text_field :name, class: "form-control", placeholder: 'Skip to let us infer from file name' %>
    </div>
    <div class="form-group">
      <%= f.label :name, "Email Column" %>
      <%= f.text_field :email_column, class: "form-control", placeholder: '* Name of the email column on the CSV', required: true %>
    </div>
   <div class="form-group">
      <%= f.file_field :email_csv, class: 'form-control-file', required: true %>
   </div>
   <%= f.submit "Upload", class: "btn btn-primary" %>
<% end %>
</div>
